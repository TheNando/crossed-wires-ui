{
  "name": "crossed-wires-ui",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html --no-cache",
    "build": "parcel build ./src/index.html -d build/ --public-url ./"
  },
  "author": "Nando Pe√±a",
  "license": "MIT",
  "dependencies": {
    "firebase": "^4.6.2",
    "typicons.font": "^2.0.9"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "mithril": "^1.1.6",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.13.4"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ],
      [
        "transform-react-jsx",
        {
          "pragma": "m"
        }
      ],
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ],
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ]
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
